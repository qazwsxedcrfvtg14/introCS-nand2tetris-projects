/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    Nand(a=sel,b=sel,out=nsel);
    Nand(a=in,b=nsel,out=c1);
    Nand(a=c1,b=c1,out=a);
    Nand(a=sel,b=in,out=c2);
    Nand(a=c2,b=c2,out=b);
}
